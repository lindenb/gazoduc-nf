nextflow_process {

    name "Test Process ANNOTSV_APPLY"
    script "modules/annotsv/apply/main.nf"
    process "ANNOTSV_APPLY"
    // tag "gitlab_ci"
    tag "modules"
    tag "modules/annotsv"
    tag "modules/annotsv/apply"

   setup {
           run("ANNOTSV_INSTALL") {
                script "modules/annotsv/install"
               
                process {
                        """
                        input[0] = [id:"annotsv"]
                        """
                        }
                }
            }



    test("BASIC") {

        when {
            params {
                // define parameters here. Example:
                // outdir = "tests/results"
            }
            process {
                """
	                input[0] = ANNOTSV_INSTALL.out.annotsv_db
                        input[1] = [
                            [id:"ref",ucsc_name:"hg19"],
                            file("${projectDir}/tests/data/rotavirus_rf.dict")
                            ]

                        input[2] = [
                            [id:"ref"],
                            file("${projectDir}/tests/data/rotavirus_rf.vcf.gz")
                            ]
                """
            }
        }

        then {
            assert process.success
            //assert snapshot(process.out).match()
        }

    }

    test("STUB") {
    	tag "gitlab_ci"
    	tag "stub"
        tag "modules/annotsv/apply_stub"
	options "-stub"
        when {
            params {
                // define parameters here. Example:
                // outdir = "tests/results"
            }
            process {
                """
	                input[0] = ANNOTSV_INSTALL.out.annotsv_db
                        input[1] = [
                            [id:"ref",ucsc_name:"hg19"],
                            file("${projectDir}/tests/data/rotavirus_rf.dict")
                            ]

                        input[2] = [
                            [id:"ref"],
                            file("${projectDir}/tests/data/rotavirus_rf.vcf.gz")
                            ]
                """
            }
        }

        then {
            assert process.success
            //assert snapshot(process.out).match()
        }

    }



}
