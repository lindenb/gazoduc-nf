nextflow_process {

    name "Test Process GROUP_BY_GENE"
    script "modules/jvarkit/groupbygene/main.nf"
    process "GROUP_BY_GENE"
    tag "gitlab_ci"
    tag "modules"
    tag "modules/jvarkit"
    tag "modules/jvarkit/groupbygene"

    test("BASIC") {

        when {
            params {
               
             }
            process {
                """
                input[0] = [ [id:"case"], []] 
                input[1] = [ [id:"ctrls"], []] 
                input[2] = [ [id:"vcf"], file("${projectDir}/tests/data/rotavirus_rf.ann.vcf.gz") ]
                """
            }
        }

        then {
            assert process.success
            //assert snapshot(process.out).match()
        }

    }

    test("STUB") {
        tag "stub"
        options "-stub"
        when {
            params {
              
            }
            process {
                """
                input[0] = [ [id:"case"], []] 
                input[1] = [ [id:"ctrls"], []] 
                input[2] = [ [id:"stub"], file("${projectDir}/tests/data/rotavirus_rf.ann.vcf.gz") ]
                """
            }
        }

        then {
            assert process.success
            //assert snapshot(process.out).match()
        }

    }
}
