nextflow_process {

    name "Test Process JVARKIT_VCFFILTERJDK"
    script "modules/jvarkit/vcffilterjdk/main.nf"
    process "JVARKIT_VCFFILTERJDK"
    config "./nextflow.config"
    tag "gitlab_ci"
    tag "modules"
    tag "modules/jvarkit"
    tag "modules/jvarkit/vcffilterjdk"

    test("BASIC") {

        when {
            params {
                // define parameters here. Example:
                // outdir = "tests/results"
            }
            process {
                """
                input[0] = [[id:"noscript"],[]]
                input[1] = [[id:"noped"],[]]
                input[2] = [[id:"vcf"],file("${projectDir}/tests/data/rotavirus_rf.vcf.gz")]
                """
            }
        }

        then {
            assert process.success
            //assert snapshot(process.out).match()
        }

    }


    test("STUB") {
