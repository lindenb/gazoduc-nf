nextflow_process {

    name "Test Process RUN_ORAD"
    script "modules/orad/run/main.nf"
    process "RUN_ORAD"
    tag "gitlab_ci"
    tag "modules"
    tag "modules/orad"
    tag "modules/orad/run"
    // config "../download/nextflow.config"

    test("STUB") {
	tag "stub"
	options "-stub"
        when {
            params {
                // define parameters here. Example:
                // outdir = "tests/results"
            }
            process {
                """
                input[0]= [ [id:"orax"], file("FAKEDIR") ]
	        input[1] = [ [id:"S1"], file("${projectDir}/tests/data/S1_S10_L008_R2_001.fastq.ora") ]
                """
            }
        }

        then {
            assert process.success
            System.err.println(process.out);
           // assert snapshot(process.out).match()
        }

    }




    test("BASIC") {

setup {
      run("DOWNLOAD_ORAD") {
		script "modules/orad/download/main.nf"
		process {
			"""
	                input[0] = [:]
			"""
			}
		}
	}


        when {
            params {
                // define parameters here. Example:
                // outdir = "tests/results"
            }
            process {
                """
                input[0]= DOWNLOAD_ORAD.out.oradir
	            input[1] = [ [id:"S1"], file("${projectDir}/tests/data/S1_S10_L008_R2_001.fastq.ora") ]
                """
            }
        }

        then {
            assert process.success
            System.err.println(process.out);
           // assert snapshot(process.out).match()
        }

    }

}
