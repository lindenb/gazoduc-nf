nextflow_process {

    name "Test Process ORAD_INFO"
    script "modules/orad/info/main.nf"
    process "ORAD_INFO"
    tag "modules"
    tag "modules/orad"
    tag "modules/orad/info"
    //config "../download/nextflow.config"


   

    test("STUB") {
        tag "gitlab_ci"
        options "-stub"


         setup {
            run("DOWNLOAD_ORAD") {
                script "modules/orad/download/main.nf"
                process {
                    """
                            input[0] = [:]
                    """
                    }
                }
            }

        when {
            params {
                // define parameters here. Example:
                // outdir = "tests/results"
            }
            process {
                """
                input[0]= DOWNLOAD_ORAD.out.oradir
	            input[1] = [[id:"S1"],file("${projectDir}/tests/data/S1_S10_L008_R2_001.fastq.ora")]
                """
            }
        }

        then {
            assert process.success
            //assert snapshot(process.out).match()
        }

    }

