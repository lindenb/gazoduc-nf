nextflow_process {

    name "Test Process JQ"
    script "modules/jq/main.nf"
    process "JQ"
    config "./nextflow.config"
    tag "gitlab_ci"
    tag "modules"
    tag "modules/jq"

    test("QUERY_IN_CONFIG") {

        when {
            process {
                """
                input[0] = [[id:"j"],file("${projectDir}/tests/data/rotavirus_rf.xhunter.catalog.json")]
                input[1] = [[id:"q"],[] ]
                """
            }
        }

        then {
            assert process.success
            //assert snapshot(process.out).match()
        }

    }

}
