nextflow_process {

    name "Test Process JVARKIT_VCFFILTERSO"
    script "modules/jvarkit/vcffilterso/main.nf"
    process "JVARKIT_VCFFILTERSO"
    tag "gitlab_ci"
    tag "modules"
    tag "modules/jvarkit"
    tag "modules/jvarkit/vcffilterso"
    config "./nextflow.config"
    
    test("BASIC") {

        when {
            params {
                // define parameters here. Example:
                // outdir = "tests/results"
            }
            process {
                """
                input[0] = [[id:"r"],file("${projectDir}/tests/data/rotavirus_rf.ann.vcf.gz")]
                """
            }
        }

        then {
            assert process.success
            //assert snapshot(process.out).match()
        }

    }

    test("STUB") {
        tag "stub"
        options "-stub"
        when {
            params {
                // define parameters here. Example:
                // outdir = "tests/results"
            }
            process {
                """
                input[0] = [[id:"r"],file("${projectDir}/tests/data/rotavirus_rf.ann.vcf.gz")]
                """
            }
        }

        then {
            assert process.success
            //assert snapshot(process.out).match()
        }

    }

