tests:
	$(MAKE) -C ../workflows/indexcov -f test.mk
	$(MAKE) -C ../workflows/manta -f test.mk
	$(MAKE) -C ../workflows/smoove -f test.mk
	$(MAKE) -C ../workflows/delly2 -f test.mk

clean:
	$(MAKE) -C ../workflows/indexcov -f test.mk clean
	$(MAKE) -C ../workflows/manta -f test.mk clean
	$(MAKE) -C ../workflows/smoove -f test.mk clean
	$(MAKE) -C ../workflows/delly2 -f test.mk clean
