nextflow_pipeline {

    name "Test Workflow main.nf"
    script "workflows/ultrarares/main.nf"
    tag "workflows/ultrarares"
    tag "gitlab_ci"
    tag "workflows"

    test("STUB") {
        options "-stub"
        when {
            params {
                fasta       = "${projectDir}/tests/data/rotavirus_rf.fa"
                samplesheet = "${projectDir}/tests/data/samplesheet.bams.01.csv"
                gnomad = "${projectDir}/tests/data/fake.gnomad.vcf.gz"

            }
        }

        then {
            assert workflow.success
        }

    }
}
